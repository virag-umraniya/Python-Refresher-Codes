def find_gcd(a, b):
    while b:
        a,b = b, a%b # Concept of tuple Unpacking
    return a
a = 34
b = 12

print("GCD is:",find_gcd(a,b))





def find_gcd(a, b):
    while b:
        a,b = b, a%b # Concept of tuple Unpacking
    return a

def find_lcm(a,b):
    gcd = find_gcd(a,b)
    lcm = (a * b) // gcd
    return lcm
a = 2
b = 3
print("LCM is:",find_lcm(a,b))





